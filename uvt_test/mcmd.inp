! Douglas Franz, Space group, USF
! all input params are defined only by the second column
! 2017

name			    mcmd_development
mode	 		    mc ! mc or md
checkpoints_option off 
input_atoms  ../pdbs/mof5+h2bssp.pdb     ! PDB ONLY 

free_volume  11595.4

potential_form 		ljespolar   polar  polar   !ljespolar 	!phast2    !!lj
xyz_traj_option on
fugacity_single h2


carbasis 25.669 25.669 25.669 90 90 90




! MONTE CARLO ONLY ======================================
ensemble		    uvt

mc_corrtime	        25
finalstep		    5000000
temperature   77 	    298  ! K
pressure      1.0
insert_factor		0.667     ! uVT: prob to insert/delete in uVT
displace_factor     2.5
rotate_angle_factor 360.0
! =======================================================
